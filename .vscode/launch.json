{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug MDBot",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/MDBot.exe",
            "args": [],
            "stopAtEntry": false,
            "cwd": "${workspaceFolder}",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${workspaceFolder}/build/Debug;${env:PATH}"
                }
            ],
            "console": "integratedTerminal",
            "logging": {
                "moduleLoad": false,
                "trace": true
            }
        }
    ]
}