{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug MDBot",
            "type": "cppvsdbg",
            "request": "launch",
            "program": "${workspaceFolder}/build/Debug/MDBot.exe",
            "args": [],
            "stopAtEntry": true,
            "cwd": "${workspaceFolder}/build/Debug",
            "environment": [
                {
                    "name": "PATH",
                    "value": "${workspaceFolder}/build/Debug;${env:PATH}"
                }
            ],
            "console": "integratedTerminal",
            "symbolSearchPath": "${workspaceFolder}/build/Debug",
            "logging": {
                "moduleLoad": true,
                "trace": true,
                "engineLogging": true,
                "programOutput": true,
                "exceptions": true
            }
        },
        {
            "type": "lldb",
            "request": "launch",
            "name": "Debug MDBot lldb",
            "program": "${workspaceFolder}/build/Debug/MDBot.exe",
            "args": [],
            "cwd": "${workspaceFolder}/build/Debug",
            "internalConsoleOptions": "neverOpen",
            "console": "integratedTerminal",
            "stopOnEntry": true
        }
    ]
}